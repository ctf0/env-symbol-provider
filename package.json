{
    "activationEvents": [
        "onStartupFinished"
    ],
    "categories": [
        "Other",
        "Programming Languages"
    ],
    "contributes": {
        "configuration": {
            "properties": {
                "envSymbolProvider.referenceSearchExclude": {
                    "default": [
                        "node_modules",
                        "coverage",
                        "dist",
                        "build",
                        "vendor",
                        "storage",
                        "public",
                        "stubs",
                        ".git",
                        ".history",
                        ".idea"
                    ],
                    "description": "folders to exclude from reference search",
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "type": "array",
                    "uniqueItems": true
                }
            },
            "title": "Env Symbol Provider"
        },
        "languages": [
            {
                "extensions": [
                    ".env"
                ],
                "id": "env"
            },
            {
                "extensions": [
                    ".env"
                ],
                "id": "dotenv"
            }
        ]
    },
    "description": "Env files Symbol Provider",
    "devDependencies": {
        "@types/node": "^24.5.2",
        "@types/vscode": "^1.90.0",
        "esbuild": "^0.25.10",
        "typescript": "^5.9.2"
    },
    "displayName": "Env Symbol Provider",
    "engines": {
        "vscode": "^1.90.0"
    },
    "icon": "img/icon.png",
    "main": "./out/main",
    "name": "env-symbol-provider",
    "publisher": "ctf0",
    "repository": "https://github.com/ctf0/env-symbol-provider",
    "scripts": {
        "esbuild": "npm run esbuild-base -- --sourcemap",
        "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
        "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
        "vscode:prepublish": "npm run esbuild-base -- --pure:console.log --minify",
        "watch": "npm run esbuild"
    },
    "version": "0.0.5"
}
